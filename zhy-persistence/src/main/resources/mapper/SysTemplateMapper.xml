<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhy.mapper.SysTemplateMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zhy.dataobject.SysTemplateDO">
        <id column="id" property="id" />
        <result column="is_valid" property="isValid" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="create_by" property="createBy" />
        <result column="gmt_modify" property="gmtModify" />
        <result column="modify_by" property="modifyBy" />
        <result column="type" property="type" />
        <result column="name" property="name" />
        <result column="shipowner_id" property="shipownerId" />
        <result column="version" property="version" />
        <result column="doc_type" property="docType" />
        <result column="remark" property="remark" />
        <result column="attach_id" property="attachId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        is_valid,
        gmt_create,
        create_by,
        gmt_modify,
        modify_by,
        id, type, name, shipowner_id, version, doc_type, remark, attach_id
    </sql>

</mapper>
